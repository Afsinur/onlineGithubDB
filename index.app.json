  "data": "\r\n      <div>\r\n        <button class=\"add_new\"><span>+</span> <span>Add new</span></button>\r\n      </div>\r\n\r\n      <div data-app-child2>\r\n        <div>\r\n          show\r\n          <select id=\"show_entries\">\r\n            <option value=\"10\">10</option>\r\n            <option value=\"20\">20</option>\r\n            <option value=\"50\">50</option>\r\n            <option value=\"100\">100</option>\r\n          </select>\r\n          entries\r\n        </div>\r\n\r\n        <div>\r\n          <label for=\"search\">search</label>\r\n          <input type=\"text\" id=\"search\" />\r\n        </div>\r\n      </div>\r\n\r\n      <div>\r\n        <div data-show-data-container></div>\r\n      </div>\r\n\r\n      <div class=\"add_new_form_container\">\r\n        <div class=\"add_new_form\">\r\n          <h2>add info</h2>\r\n\r\n          <form data-add-new-form>\r\n            <div>\r\n              <div>\r\n                <label for=\"name_ban\">নাম</label>\r\n                <input type=\"text\" id=\"name_ban\" />\r\n                <div class=\"error\" data-name-ban></div>\r\n              </div>\r\n\r\n              <div>\r\n                <label for=\"name_eng\">Name</label>\r\n                <input type=\"text\" id=\"name_eng\" />\r\n                <div class=\"error\" data-name-eng></div>\r\n              </div>\r\n            </div>\r\n\r\n            <div>\r\n              <div>\r\n                <label for=\"name_father_ban\">পিতার নাম</label>\r\n                <input type=\"text\" id=\"name_father_ban\" />\r\n                <div class=\"error\" data-name-father-ban></div>\r\n              </div>\r\n\r\n              <div>\r\n                <label for=\"name_mother_ban\">মাতার নাম</label>\r\n                <input type=\"text\" id=\"name_mother_ban\" />\r\n                <div class=\"error\" data-name-mother-ban></div>\r\n              </div>\r\n            </div>\r\n\r\n            <div>\r\n              <div>\r\n                <label for=\"date_of_birth\">date of birth</label>\r\n                <input type=\"date\" id=\"date_of_birth\" />\r\n                <div class=\"error\" data-date-of-birth></div>\r\n              </div>\r\n\r\n              <div>\r\n                <label for=\"NID_no\">NID</label>\r\n                <input type=\"text\" id=\"NID_no\" />\r\n                <div class=\"error\" data-nid-no></div>\r\n              </div>\r\n            </div>\r\n\r\n            <div>\r\n              <div>\r\n                <label for=\"pin_no\">PIN no</label>\r\n                <input type=\"text\" id=\"pin_no\" />\r\n                <div class=\"error\" data-pin-no></div>\r\n              </div>\r\n\r\n              <div>\r\n                <label for=\"blood_group\">রক্তের গ্রুপ</label>\r\n                <input type=\"text\" id=\"blood_group\" />\r\n                <div class=\"error\" data-blood-group></div>\r\n              </div>\r\n            </div>\r\n\r\n            <div>\r\n              <div>\r\n                <label for=\"birth_place\">জন্মস্থান</label>\r\n                <input type=\"text\" id=\"birth_place\" />\r\n                <div class=\"error\" data-birth-place></div>\r\n              </div>\r\n\r\n              <div>\r\n                <label for=\"nid_given_date\">প্রদানের তারিখ</label>\r\n                <input type=\"date\" id=\"nid_given_date\" />\r\n                <div class=\"error\" data-nid-given-date></div>\r\n              </div>\r\n            </div>\r\n\r\n            <div>\r\n              <div>\r\n                <label for=\"home_address\">বাসা/হোল্ডিং</label>\r\n                <input type=\"text\" id=\"home_address\" />\r\n                <div class=\"error\" data-home-address></div>\r\n              </div>\r\n\r\n              <div>\r\n                <label for=\"street_address\">গ্রাম/রাস্তা</label>\r\n                <input type=\"text\" id=\"street_address\" />\r\n                <div class=\"error\" data-street-address></div>\r\n              </div>\r\n            </div>\r\n\r\n            <div>\r\n              <div>\r\n                <label for=\"nid_photo\">ছবি</label>\r\n                <input type=\"file\" id=\"nid_photo\" />\r\n                <div class=\"error\" data-nid-photo></div>\r\n              </div>\r\n\r\n              <div>\r\n                <label for=\"signature\">স্বাক্ষর</label>\r\n                <input type=\"file\" id=\"signature\" />\r\n                <div class=\"error\" data-signature></div>\r\n              </div>\r\n            </div>\r\n\r\n            <div data-add-button-container>\r\n              <button data-add-cancel>Cancel</button>\r\n              <button type=\"submit\" data-update-or-add>Add</button>\r\n            </div>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    "
